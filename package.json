{
  "name": "komondor-plugin-ws",
  "description": "komondor plugin for ws",
  "version": "0.0.0-development",
  "main": "dist-es5/index.js",
  "typings": "dist-es5/index.d.ts",
  "files": [
    "dist-es5",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-es5",
    "build-es5": "tsc -p tsconfig.es5.json",
    "clean": "rimraf dist-es5",
    "codecov": "codecov",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dependency-check": "dependency-check . --unused --no-dev -i ws && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "preversion": "git pull && npm run verify",
    "test": "jest",
    "verify": "npm run lint && npm run coverage && npm run build && npm run dependency-check",
    "watch": "node scripts/watch.js",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/komondor-plugin-ws.git"
  },
  "bugs": {
    "url": "https://github.com/unional/komondor-plugin-ws/issues"
  },
  "homepage": "https://github.com/unional/komondor-plugin-ws",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "jest": {
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": ".*.(test|spec).(js|ts)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "peerDependencies": {
    "komondor": ">=4.7.1",
    "ws": ">=4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.5",
    "dependency-check": "^3.0.0",
    "jest": "^22.2.2",
    "komondor": "^4.7.1",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.0.0",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "tslint-config-unional": "^0.9.0",
    "typescript": "^2.8.0-dev.20180211",
    "ws": "^5.0.0"
  },
  "dependencies": {
    "satisfier": "^4.1.1"
  }
}
